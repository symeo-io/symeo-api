{
  "name": "symeo-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "yarn config:build && nest build",
    "config:build": "symeo-js build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"tests/**/*.ts\\\"",
    "start": "symeo-js start -- nest start --watch",
    "start:debug": "symeo-js start -- nest start --debug --watch",
    "start:prod": "symeo-js start -- node dist/main",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "migration:create": "typeorm migration:create",
    "test": "symeo-js start -f symeo.test.yml -- jest --maxWorkers=1",
    "test:watch": "symeo-js start -f symeo.test.yml -- jest --watch --maxWorkers=1",
    "test:cov": "symeo-js start -f symeo.test.yml -- jest --coverage --maxWorkers=1"
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/terminus": "^9.2.0",
    "@nestjs/typeorm": "^9.0.1",
    "auth0": "^3.1.2",
    "aws-sdk": "^2.1303.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dd-trace": "^3.13.2",
    "jwks-rsa": "^3.0.1",
    "lodash": "^4.17.21",
    "nest-winston": "^1.8.0",
    "nodejs-base64": "^2.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "request-context": "^2.0.0",
    "rxjs": "^7.2.0",
    "symeo-js": "^0.0.8",
    "ts-mockito": "^2.6.1",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/auth0": "^2.35.9",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios-mock-adapter": "^1.21.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.4.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "testcontainers": "^9.1.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  }
}
